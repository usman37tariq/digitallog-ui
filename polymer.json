  "sources": [
    "manifest.json",
    "package.json",
    "server/*.js",
    "public/favicon.*",
    "public/*.*",
    "public/elements/**/*",
    "public/images/**/*",
    "public/bower_components/webcomponentsjs/*.min.js",
    "public/bower_components/jquery/dist/*.min.js",
    "public/bower_components/jquery-ui/**/*.js",
    "public/bower_components/jquery-ui/themes/base/*",
    "public/bower_components/bootstrap/dist/**/*",
    "public/bower_components/fontawesome/css/*.min.css",
    "public/bower_components/fontawesome/webfonts/*",
    "public/bower_components/jstree/dist/**/*",
    "public/bower_components/jstree/dist/themes/default/*",
    "public/bower_components/highcharts/*.js",
    "public/bower_components/highcharts/modules/*.js",
    "public/bower_components/daterangepicker/*",
    "public/bower_components/moment/min/**/*",
    "public/bower_components/select2/dist/**/*",
    "public/bower_components/offline/**/*",
    "public/bower_components/datatables.net/**/*",
    "public/bower_components/datatables.net-bs4/**/*",
    "public/bower_components/datatables.net-responsive/**/*",
    "public/bower_components/datatables.net-responsive-bs4/**/*"
  ],
  "builds": [
    {
      "name": "digital-log",
      "js": {
        "compile": {
          "exclude": ["*.min.js"]
        },
        "minify": {
          "exclude": ["*.min.js"]
        }
      },
      "css": {
        "minify": {
          "exclude": ["*.min.css"]
        }
      },
      "html": {
        "minify": true
      },
      "bundle": true
    }
  ]
}
